  "scripts": {
    "link": "node ./scripts/link.js",
    "unlink": "node ./scripts/link.js -u",
    "start": "react-scripts-ts start",
    "start:linked": "react-app-rewired start",
    "build": "react-scripts-ts build",
    "build:ci": "yarn build && yarn run test:ci",
    "test": "react-scripts-ts test --env=jsdom",
    "test:ci": "yarn lint && yarn test:coverage",
    "test:coverage": "CI=true react-scripts-ts test --env=jsdom --coverage --testResultsProcessor ./node_modules/jest-junit",
    "eject": "react-scripts-ts eject",
    "prepush": "yarn test:ci",
    "prettier-opts": "prettier --write",
    "prettier": "yarn prettier-opts \"./src/**/*.{js,jsx,ts,tsx}\"",
    "ci": "yarn lint && yarn test:ci",
    "lint": "yarn tslint && yarn jslint",
    "jslint": "node node_modules/eslint/bin/eslint.js src --max-warnings=0",
    "tslint": "tslint -p . -c ./tslint.json",
    "patch": "npm version patch",
    "major": "npm version major",
    "minor": "npm version minor",
    "set-version": "node ./version"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost:8080",
      "pathRewrite": {
        "^/api": "/"
      }
    },
    "/pi-api": {
      "target": "http://localhost:3000",
      "pathRewrite": {
        "^/pi-api": "/"
      }
    },
    "/eConecta": {
      "target": "http://localhost:8080"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "react-i18next": "<rootDir>/__mocks__/react-i18nextmock.tsx",
      "redux-persist/lib/integration/react": "<rootDir>/__mocks__/redux-persist-integration-mock.tsx"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts*",
      "!**/*.types.{js,ts}",
      "!**/assets/**",
      "!**/*.test.*",
      "!src/**/*.container.*",
      "!src/**/bundles.*",
      "!src/**/index.*",
      "!src/**/*.style.*",
      "!src/**/routes.*",
      "!src/**/bundleg.*",
      "!src/**/app.js",
      "!src/**/reducer.js",
      "!src/**/theme.js",
      "!src/**/inject-script.js",
      "!src/**/store.js"
    ]
  }
